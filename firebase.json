{
  "projects": {
    "default": "tuntas-kilat"
  },
  "firestore": {
    "database": "(default)",
    "location": "asia-east1",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },

  "apphosting": {
    "backendId": "tuntas-kilat-app",
    "rootDir": "/",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      ".env"
    ]
  },
  "storage": {
    "rules": "storage.rules"
  }
}
